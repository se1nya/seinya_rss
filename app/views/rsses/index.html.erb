<p id="notice"><%= notice %></p>

<h1>Listing Seinya RSS</h1>

<table>
  <thead>
    <tr>
      <th>URL</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rsses.each do |rss| %>
      <tr>
        <td><%= rss.url %></td>
        <td><%= rss.category %></td>
        <td><%= link_to 'Show', rss %></td>
        <td><%= link_to 'Edit', edit_rss_path(rss) %></td>
        <td><%= link_to 'Delete', rss, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rss URL', new_rss_path %>
